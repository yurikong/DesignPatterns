public class FoodBar extends Room{
    
    Room room;
  public FoodBar(Room room){
     this.room= room;
  }

    @Override
    public int getPrice() {
        return 10 + room.getPrice();
    }

    
    public String getDescription() {
        return room.getRoom_number()+" "+ room.getType() +", Food Bar";
    }
    
  
}

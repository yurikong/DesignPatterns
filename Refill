public class Refill extends Room{
    Room room;
  public Refill(Room room){
     this.room= room;
  }

    @Override
    public int getPrice() {
        return 5 + room.getPrice();
    }


    public String getDescription() {
        return room.getRoom_number()+" "+ room.getType() +", AutoRefill";
    }
    
}
